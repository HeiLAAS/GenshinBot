{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "OpqApiSettings": {
    // 将开头部分换成你的OPQ地址和端口，/v1/LuaApiCaller不用动
    "HttpApi": "http://localhost:8888/v1/LuaApiCaller",
    // 替换地址和端口为你的OPQ地址和端口，前面的ws://不要动
    "WebSocketApi": "ws://localhost:8888",
    // 替换为你的机器人QQ号
    "LoginQQ": "123456"
  },
  "ConnectionStrings": {
    "DbConn": "Data Source=GenshinBot.db"
  },
  "MsgQueueSettings": {
    "DelayAfterSent": 1000,
    "Capacity": 100
  }
}